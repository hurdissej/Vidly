@model IEnumerable<Vidly.Models.Movie>
    
@{
    ViewBag.Title = "Random";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table style="width: 300px; margin-top: 50px;" class="table table-striped table-hover ">
    <thead>
    <tr>
        <th> Movie </th>
    </tr>
    </thead>
    @foreach (var movie in @Model)
    {
        <tr>
            <td> @movie.Name  </td>
        </tr>
    }

</table>